 <div class="container">
     <div class="row">
        <% flash.each do |name, msg| %>
    <div class='alert alert-<%= "#{name}" %> fade in'>
       <button type="button" class="close" data-dismiss="alert">&times;</button>
        <%= content_tag :div,msg, :id =>"flash_#{name}" if msg.is_a?(String) %>
    </div>
<!--         <ul><li><%= msg %></li></ul>-->
  <% end %>
     </div>
</div>